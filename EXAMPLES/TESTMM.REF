a1 start
  entry go
  extrn opnput,put(libput),clsput
go = <opnput /1/'a.a'> <a> <b> <c> <clsput>
a  = <put /1/>     <put /1/(())> <put/1/' '><put/1/' '> +
                   <PUT/1/' '>  <put /1/'ab'> 
b  = <put/1/ 'a\nb'>
c  = <put/1/ '111111111111111111111111111111111111111111111111111' +
          '222222222222222222222222222222222222222222222222222' +
          '333333333333333333333333333333333333333333333333333'>
 end
